      "source": [
        "def quick_sort(arr):\n",
        "    if len(arr) <= 1:\n",
        "        return arr\n",
        "    pivot = arr[0]  # 选择第一个元素作为基准\n",
        "    left = [x for x in arr[1:] if x < pivot]  # 比基准小的放左边\n",
        "    right = [x for x in arr[1:] if x >= pivot]  # 比基准大的放右边\n",
        "    return quick_sort(left) + [pivot] + quick_sort(right)\n",
        "\n"
      ]
    }
  ],
